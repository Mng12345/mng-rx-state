{
  "name": "mng-rx-state",
  "version": "0.1.0",
  "description": "use rxjs to make a react state manager",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npx webpack serve",
    "build:cjs": "tsc -d --module commonjs --target es5 --outDir dist/cjs",
    "build:esm": "tsc -d --module es2015 --target es5 --outDir dist/esm",
    "build": "npm run build:cjs & npm run build:esm"
  },
  "keywords": [
    "rxjs",
    "state",
    "manager",
    "react"
  ],
  "author": "mng12345",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "html-webpack-plugin": "^5.2.0",
    "prettier": "^2.2.1",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@types/react-dom": "^17.0.1",
    "lodash": "^4.17.21",
    "mng-easy-util": "^1.4.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rxjs": "^6.6.3"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
